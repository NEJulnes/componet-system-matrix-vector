{
  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}\\tests.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [ "--trace-warnings" ]
    }
  ]
}